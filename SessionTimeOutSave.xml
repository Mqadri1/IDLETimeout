<?xml version="1.0" encoding="utf-8"?>
<widget id="avertra corp.sessiontimeoutsave.SessionTimeOutSave" pluginWidget="true" needsEntityContext="true" offlineCapable="true"
        supportedPlatform="Web"
        xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Session Timeout</name>
    <description>This widget is to force logout if the user didn't use the tool with a specific time with saving the data based on the class name</description>
    <icon></icon>
    <properties>
    
        <propertyGroup caption="General">
       
            <property key="idleTimeout" type="integer" required="true" defaultValue="10">
                <caption>Timeout Clock</caption>
                <description>Amount of time (in minutes) to consider for the session timeout, it should be larger or equal to 1.</description>
            </property>
            <!-- this is for the alert message when the timer reachs-->
            <property key="alertTitle" type="string" required="true" defaultValue="You Have Been Idle!">
                <caption>Alert Title</caption>
                <description>short title for modal alert that will display.</description>
            </property>
            <!-- this is for the IDLE time out popup to be visable -->   
            <property key="alertTimeout" type="integer" required="true"  defaultValue="30">
                <caption>Alert Timer</caption>
                <description>Amount of time (in seconds) to wait for user action to occur before action executes, it should be larger or equal to 10.</description>
            </property>
            <!-- this is for the Signout and save button the will be shown with the popup-->

            <property key="signOutButton" type="string" required="true" defaultValue="Sign out and save">
                <caption>Signout button caption</caption>
                <description>the caption of the Signout and save  button, that will lougout the user and save his data.</description>
            </property>
            <!-- this is for the Stay button the will be shown with the popup-->

            <property key="stayButton" type="string" required="true" defaultValue="Stay">
                <caption>Stay button caption</caption>
                <description>the caption of the Stay button, that will keep the user in the session.</description>
            </property>
           
        </propertyGroup>

            <!-- this is for the class name that will be on the microflow that will be triggerd if the user clicked on signed out or got loggedout because of the ideal timmer-->

        <propertyGroup caption="Behavior">
         <property key="classID" type="string" required="true" defaultValue="SessionTimeOutSave">
                <caption>Class ID</caption>
                <description>the class id that will trigger the microflow, if the class is not used in the page it will logout without saving the data.</description>
            </property>

        </propertyGroup>

            </properties>

</widget>